<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/fragmentMenu">

    <fragment
        android:id="@+id/fragmentMenu"
        android:name="com.miklesam.applicationbeerapi.FragmentMenu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_fragmentMenu_to_fragmentRandomBeer"
            app:destination="@id/fragmentRandomBeer" />
        <action
            android:id="@+id/action_fragmentMenu_to_fragmentCategotyBeer"
            app:destination="@id/fragmentCategotyBeer" />
    </fragment>
    <fragment
        android:id="@+id/fragmentRandomBeer"
        android:name="com.miklesam.applicationbeerapi.FragmentRandomBeer"
        android:label="FragmentRandomBeer" />
    <fragment
        android:id="@+id/fragmentCategotyBeer"
        android:name="com.miklesam.applicationbeerapi.FragmentCategotyBeer"
        android:label="FragmentCategotyBeer" />
</navigation>